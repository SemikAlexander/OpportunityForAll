<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/onboardFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.opportunityforall.ui.fragments.LoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
        <action
            android:id="@+id/action_loginFragment_to_helloFragment"
            app:destination="@id/helloFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.example.opportunityforall.ui.fragments.SignUpFragment"
        android:label="SignUpFragment"
        tools:layout="@layout/fragment_sign_up">
        <action
            android:id="@+id/action_signUpFragment_to_athleteFragment1"
            app:destination="@id/athleteFragment1" />
        <action
            android:id="@+id/action_signUpFragment_to_communityMemberFragment1"
            app:destination="@id/communityMemberFragment1" />
    </fragment>
    <fragment
        android:id="@+id/communityMemberFragment1"
        android:name="com.example.opportunityforall.ui.fragments.community.sign_up.CommunityMemberFragment1"
        android:label="fragment_community_member1"
        tools:layout="@layout/fragment_community_member1" >
        <action
            android:id="@+id/action_communityMemberFragment1_to_communityMemberFragment2"
            app:destination="@id/communityMemberFragment2" />
    </fragment>
    <fragment
        android:id="@+id/communityMemberFragment2"
        android:name="com.example.opportunityforall.ui.fragments.community.sign_up.CommunityMemberFragment2"
        android:label="fragment_community_member2"
        tools:layout="@layout/fragment_community_member2" />
    <fragment
        android:id="@+id/athleteFragment1"
        android:name="com.example.opportunityforall.ui.fragments.athlete.AthleteFragment1"
        android:label="fragment_athlete1"
        tools:layout="@layout/fragment_athlete1" >
        <action
            android:id="@+id/action_athleteFragment1_to_athleteFragment2"
            app:destination="@id/athleteFragment2" />
    </fragment>
    <fragment
        android:id="@+id/athleteFragment2"
        android:name="com.example.opportunityforall.ui.fragments.athlete.AthleteFragment2"
        android:label="fragment_athlete2"
        tools:layout="@layout/fragment_athlete2" >
        <action
            android:id="@+id/action_athleteFragment2_to_athleteFragment3"
            app:destination="@id/athleteFragment3" />
    </fragment>
    <fragment
        android:id="@+id/athleteFragment3"
        android:name="com.example.opportunityforall.ui.fragments.athlete.AthleteFragment3"
        android:label="fragment_athlete3"
        tools:layout="@layout/fragment_athlete3" >
        <action
            android:id="@+id/action_athleteFragment3_to_athleteFragment4"
            app:destination="@id/athleteFragment4" />
    </fragment>
    <fragment
        android:id="@+id/athleteFragment4"
        android:name="com.example.opportunityforall.ui.fragments.athlete.AthleteFragment4"
        android:label="fragment_athlete4"
        tools:layout="@layout/fragment_athlete4" >
        <action
            android:id="@+id/action_athleteFragment4_to_athleteFragment5"
            app:destination="@id/athleteFragment5" />
    </fragment>
    <fragment
        android:id="@+id/athleteFragment5"
        android:name="com.example.opportunityforall.ui.fragments.athlete.AthleteFragment5"
        android:label="fragment_athlete5"
        tools:layout="@layout/fragment_athlete5" />
    <fragment
        android:id="@+id/onboardFragment"
        android:name="com.example.opportunityforall.ui.fragments.OnboardFragment"
        android:label="OnboardFragment"
        tools:layout="@layout/fragment_onboard" >
        <action
            android:id="@+id/action_onboardFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>

    <fragment
        android:id="@+id/helloFragment"
        android:name="com.example.opportunityforall.ui.fragments.community.actions.HelloFragment"
        android:label="fragment_hello"
        tools:layout="@layout/fragment_hello" >
        <action
            android:id="@+id/action_helloFragment_to_infoAthleteFragment"
            app:destination="@id/infoAthleteFragment" />
    </fragment>
    <fragment
        android:id="@+id/infoAthleteFragment"
        android:name="com.example.opportunityforall.ui.fragments.community.actions.InfoAthleteFragment"
        android:label="fragment_info_athlete"
        tools:layout="@layout/fragment_info_athlete" >
        <action
            android:id="@+id/action_infoAthleteFragment_to_getStartedFragment"
            app:destination="@id/getStartedFragment" />
    </fragment>
    <fragment
        android:id="@+id/getStartedFragment"
        android:name="com.example.opportunityforall.ui.fragments.community.actions.flow.GetStartedFragment"
        android:label="fragment_get_started"
        tools:layout="@layout/fragment_get_started" >
        <action
            android:id="@+id/action_getStartedFragment_to_flowFragment1"
            app:destination="@id/flowFragment1" />
    </fragment>
    <fragment
        android:id="@+id/flowFragment1"
        android:name="com.example.opportunityforall.ui.fragments.community.actions.flow.FlowFragment1"
        android:label="fragment_flow1"
        tools:layout="@layout/fragment_flow1" >
        <action
            android:id="@+id/action_flowFragment1_to_flowFragment2"
