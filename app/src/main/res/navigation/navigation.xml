            android:id="@+id/action_flowFragment2_to_flowFragment3"
            app:destination="@id/flowFragment3" />
    </fragment>
    <fragment
        android:id="@+id/flowFragment3"
        android:name="com.example.opportunityforall.ui.fragments.community.actions.flow.FlowFragment3"
        android:label="fragment_flow3"
        tools:layout="@layout/fragment_flow3" >
        <action
            android:id="@+id/action_flowFragment3_to_flowFragment4"
            app:destination="@id/flowFragment4" />
    </fragment>
    <fragment
        android:id="@+id/flowFragment4"
        android:name="com.example.opportunityforall.ui.fragments.community.actions.flow.FlowFragment4"
        android:label="fragment_flow4"
        tools:layout="@layout/fragment_flow4" >
        <action
            android:id="@+id/action_flowFragment4_to_flowFragment5"
            app:destination="@id/flowFragment5" />
    </fragment>
    <fragment
        android:id="@+id/flowFragment5"
        android:name="com.example.opportunityforall.ui.fragments.community.actions.flow.FlowFragment5"
        android:label="fragment_flow5"
        tools:layout="@layout/fragment_flow5" >
        <action
            android:id="@+id/action_flowFragment5_to_flowFragment6"
            app:destination="@id/flowFragment6" />
    </fragment>
    <fragment
        android:id="@+id/flowFragment6"
        android:name="com.example.opportunityforall.ui.fragments.community.actions.flow.FlowFragment6"
        android:label="fragment_flow6"
        tools:layout="@layout/fragment_flow6" />
</navigation>